{{page-title "Add"}}
<div class="row">
  <div class="col">
    <Input @value={{@model.nom}} placeholder="Entrez votre nom" class="form-control"/>
  </div><div class="col">
  <Input @value={{@model.prenom}} placeholder="Entrez votre prenom" class="form-control"/>
</div><div class="col">
  <Input @value={{@model.email}} placeholder="Entrez votre email" type="email" class="form-control"/>
</div>
  <button class="btn btn-primary" {{action "save" @model}}> Enregister </button>
  <button class="btn btn-secondary" {{action "annuler"}}>Annuler</button>
</div>
{{outlet}}